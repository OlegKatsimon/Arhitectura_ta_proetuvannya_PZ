// main.cpp
#include <iostream>
#include "CalculatorViewModel.h"

int main() {
    CalculatorViewModel viewModel;
    double a, b;

    std::cout << "Введите первое число: ";
    std::cin >> a;
    std::cout << "Введите второе число: ";
    std::cin >> b;

    viewModel.performAddition(a, b);

    return 0;
}
